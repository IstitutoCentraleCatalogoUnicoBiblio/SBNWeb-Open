			<xsd:documentation>tipo persona (Fisica, Giuridica)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:simpleType name="dsType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="80"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="flagType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="tab">
		<xsd:annotation>
			<xsd:documentation>tabelle tipizzate</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="tabRegione" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>tabella con elenco delle regioni</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:all>
							<xsd:element ref="idRegione"/>
							<xsd:element name="dsRegione" type="dsTabType">
								<xsd:annotation>
									<xsd:documentation>descrizione</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="tabProvincia" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>tabella con elenco delle provincie</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:all>
							<xsd:element ref="cdIstat"/>
							<xsd:element name="dsProvincia" type="dsTabType">
								<xsd:annotation>
									<xsd:documentation>descrizione</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="sigla">
								<xsd:annotation>
									<xsd:documentation>sigla della provincia</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="2"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element ref="idProvincia"/>
							<xsd:element ref="idRegione"/>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="tabLingua" minOccurs="0" maxOccurs="unbounded">

					<xsd:documentation>forma dell'opera</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="d_231" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>data dell'opera</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="k_231" type="xsd:string" minOccurs="0">
				<xsd:annotation>

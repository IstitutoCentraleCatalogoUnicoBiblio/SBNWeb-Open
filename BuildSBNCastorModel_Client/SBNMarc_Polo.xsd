					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="flagType"/>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element ref="cdRecSebina4" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>

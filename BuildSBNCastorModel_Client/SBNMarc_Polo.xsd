								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="3"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="cd6392t" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="3"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="cd6391" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="2"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="fl_valido_636">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="1"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="dsFonteLingua" type="dsType">
								<xsd:annotation>
									<xsd:documentation>descrizione</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ntLingua" type="noteType" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>note</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="tabPaese" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>tabella con elenco dei paesi</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:all>
							<xsd:element ref="codice"/>
							<xsd:element ref="idPaese" minOccurs="0"/>
							<xsd:element name="cd31661a2" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="2"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="cd31661a3" minOccurs="0">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="3"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="cd31661n3" type="xsd:integer" minOccurs="0"/>
							<xsd:element name="fl_valido_3166" type="flagType"/>
							<xsd:element name="dsFontePaese">
								<xsd:annotation>
									<xsd:documentation>descrizione</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="80"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="ntPaese" type="noteType" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>note</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="tabComune" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>tabella con elenco dei comuni</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:all>
							<xsd:element ref="cdIstat"/>
							<xsd:element name="dsComune" type="dsType">
								<xsd:annotation>
									<xsd:documentation>descrizione</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element ref="idProvincia"/>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="tabDescI18N" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>tabella con descrizioni I18N</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:all>
							<xsd:element name="cdTipoTabella" type="cdType">
								<xsd:annotation>
									<xsd:documentation>codice tipo tabella</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element ref="idElemento"/>
							<xsd:element ref="idLingua"/>
							<xsd:element ref="idPaese"/>
							<xsd:element ref="dsElemento"/>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="tabTitStu" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>tabella con elenco titoli di studio (I18N)</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:all>
							<xsd:element ref="idElemento"/>
							<xsd:element ref="cdElemento"/>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="tabTpUte" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>tabella con elenco dei tipi utente (I18N)</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:all>
							<xsd:element ref="idElemento"/>
							<xsd:element ref="cdElemento"/>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="tabTpProv" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>tabella con elenco dei tipi provenienza (I18N)</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:all>
							<xsd:element ref="idElemento"/>
							<xsd:element ref="cdElemento"/>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="tabTpBbr" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>tabella con elenco dei tipi bibliotecario</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:all>
							<xsd:element ref="idElemento"/>
							<xsd:element ref="cdElemento"/>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="tabAreaInter" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>tabella con elenco aree di interesse</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:all>
							<xsd:element ref="idElemento"/>
							<xsd:element ref="cdElemento"/>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="tabModFet" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>tabella con elenco modelli fascie eta</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="idElemento"/>
							<xsd:element ref="cdElemento"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="tabFascEta" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>tabella con elenco fascie eta</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="idFasciaEta" type="xsd:integer"/>
							<xsd:element name="idModello" type="xsd:integer"/>
							<xsd:element name="cdFasciaEta" type="cdType"/>
							<xsd:element name="etaIni" type="xsd:integer"/>
							<xsd:element name="etaFin" type="xsd:integer"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="lingua">
		<xsd:annotation>
			<xsd:documentation>lingua</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:length value="2"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="paese">
		<xsd:annotation>
			<xsd:documentation>paese</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:length value="2"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="descr" type="dsType">
		<xsd:annotation>
			<xsd:documentation>descrizione</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="nomeTab">
		<xsd:annotation>
			<xsd:documentation>nome tabella</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:minLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="cdIstat" type="cdType">
		<xsd:annotation>
			<xsd:documentation>codice istat</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="dsTab" type="dsTabType">
		<xsd:annotation>
			<xsd:documentation>descrizione</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:simpleType name="idType">
		<xsd:restriction base="xsd:integer"/>
	</xsd:simpleType>
	<xsd:simpleType name="dsTabType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="30"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="idRegione" type="idType">
		<xsd:annotation>
			<xsd:documentation>id della regione</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:simpleType name="noteType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="320"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="idProvincia" type="xsd:integer">
		<xsd:annotation>
			<xsd:documentation>id della provincia</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="default" type="flagType">
		<xsd:annotation>
			<xsd:documentation>default</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="idPaese" type="xsd:integer">
		<xsd:annotation>
			<xsd:documentation>id paese</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="idLingua" type="xsd:integer">
		<xsd:annotation>
			<xsd:documentation>id lingua</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="idElemento" type="xsd:integer">
		<xsd:annotation>
			<xsd:documentation>id elemento</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="dsElemento" type="dsType">
		<xsd:annotation>
			<xsd:documentation>descrizione elemento</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cdElemento" type="cdType">
		<xsd:annotation>
			<xsd:documentation>codice elemento</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="gue">
		<xsd:annotation>
			<xsd:documentation>guest</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="datiAna"/>
				<xsd:element ref="cdGue"/>
				<xsd:element ref="indirizzo"/>
				<xsd:element ref="email" minOccurs="0"/>
				<xsd:element ref="matricola" minOccurs="0"/>
				<xsd:element ref="codFisc" minOccurs="0"/>
				<xsd:element ref="identita" minOccurs="0"/>
				<xsd:element ref="cdTpUte"/>
				<xsd:element ref="cdTitStu" minOccurs="0"/>
				<xsd:element name="polGue" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>relazione tra il polo e il guest</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:complexContent>
							<xsd:extension base="relType">
								<xsd:sequence>
									<xsd:element ref="cdPol"/>
								</xsd:sequence>
							</xsd:extension>
						</xsd:complexContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="bibGue" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>relazione tra la biblioteca e il guest</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:complexContent>
							<xsd:extension base="relType">
								<xsd:sequence>
									<xsd:element ref="cdBiblioteca"/>
								</xsd:sequence>
							</xsd:extension>
						</xsd:complexContent>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="relType">
		<xsd:sequence>
			<xsd:element ref="note" minOccurs="0"/>
			<xsd:element ref="flAmm"/>
			<xsd:element ref="flRef"/>
			<xsd:element ref="dtRel"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="flRef" type="flagType">
		<xsd:annotation>
			<xsd:documentation>flag entit√† collegata riferimento</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="flAmm" type="flagType">
		<xsd:annotation>
			<xsd:documentation>flag entit√† collegata amministratore</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cdTpUte" type="cdType">
		<xsd:annotation>
			<xsd:documentation>codice tipo utente</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cdTpPro" type="cdType">
		<xsd:annotation>
			<xsd:documentation>codice tipo provenienza</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cdPolBib" type="cdType"/>
	<xsd:complexType name="cdBibType">
		<xsd:sequence>
			<xsd:element ref="cdPol"/>
			<xsd:element ref="cdBib"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="cdBiblioteca" type="cdBibType">
		<xsd:annotation>
			<xsd:documentation>codice biblioteca</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cdTitStu" type="cdType">
		<xsd:annotation>
			<xsd:documentation>titolo di studio</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="codFisc">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="16"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="piva">
		<xsd:annotation>
			<xsd:documentation>non utilizzato</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="11"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="matricola">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="20"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="altroCod">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="20"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="identita">
		<xsd:annotation>
			<xsd:documentation>X UTENTE: 1 doc identit√†   X BIB: 0-n Codici Biblioteca X GUEST: 0-1 doc identit√†</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:all>
				<xsd:element ref="codice" minOccurs="0"/>
				<xsd:element ref="numero" minOccurs="0"/>
			</xsd:all>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="sts">
		<xsd:annotation>
			<xsd:documentation>sottosistema</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:all>
				<xsd:element ref="datiAna"/>
				<xsd:element ref="cdSts"/>
				<xsd:element name="fl_sts_pol">
					<xsd:annotation>
						<xsd:documentation>flag sottosistema del polo</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:length value="1"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element ref="note" minOccurs="0"/>
			</xsd:all>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="fl_canc_ana" type="flagType">
		<xsd:annotation>
			<xsd:documentation>flag anagrafica cancellata</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="fl_canc_ent" type="flagType">
		<xsd:annotation>
			<xsd:documentation>flag entit√† cancellata</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="nomeRicerca">
		<xsd:annotation>
			<xsd:documentation>cognome e nome normalizzato</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:minLength value="1"/>
				<xsd:maxLength value="80"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="userName">
		<xsd:annotation>
			<xsd:documentation>username. Utilizzato solo per import da Sebina4 - per Utente Bibliotecario e Guest</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:minLength value="0"/>
				<xsd:maxLength value="80"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="password">
		<xsd:annotation>
			<xsd:documentation>password. Utilizzato solo per import da Sebina4 - per Utente Bibliotecario e Guest</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:minLength value="0"/>
				<xsd:maxLength value="80"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="grp">
		<xsd:annotation>
			<xsd:documentation>gruppi di utenti</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="datiAna"/>
				<xsd:element ref="cdGrp"/>
				<xsd:element ref="note" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="grb">
		<xsd:annotation>
			<xsd:documentation>gruppi di bibliotecari</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="datiAna"/>
				<xsd:element ref="cdGrb"/>
				<xsd:element ref="note" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="cdGrb" type="cdType">
		<xsd:annotation>
			<xsd:documentation>codice gruppo di bibliotecari</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cdAin" type="cdType">
		<xsd:annotation>
			<xsd:documentation>codice area di interesse</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cfg">
		<xsd:annotation>
			<xsd:documentation>configurazioni</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="CGrpUte" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="CGrpUte">
		<xsd:annotation>
			<xsd:documentation>configurazione automatismi gruppo-utenti</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="erogatore"/>
				<xsd:element ref="cdTpUte" minOccurs="0"/>
				<xsd:element ref="cdTpPro" minOccurs="0"/>
				<xsd:element ref="cdGrp"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="erogatore">
		<xsd:annotation>
			<xsd:documentation>Biblioteca o Polo per il quale √® definita la configurazione</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="cdPol" minOccurs="0"/>
				<xsd:element ref="cdBiblioteca" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="frn">
		<xsd:annotation>
			<xsd:documentation>fornitori</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="datiAna"/>
				<xsd:element ref="indirizzo"/>
				<xsd:element ref="email" minOccurs="0"/>
				<xsd:element ref="CFPiva" minOccurs="0"/>
				<xsd:element ref="cdFrn"/>
				<xsd:element ref="note" minOccurs="0"/>
				<xsd:element ref="cdTpFrn" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="cdFrn" type="cdType">
		<xsd:annotation>
			<xsd:documentation>codice fornitore</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cdTpFF" type="cdType">
		<xsd:annotation>
			<xsd:documentation>cd tipo formato file</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cdTpFrn" type="cdType">
		<xsd:annotation>
			<xsd:documentation>cd tipo fornitore (editore, libraio, ecc.)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="flRefOS" type="flagType">
		<xsd:annotation>
			<xsd:documentation>flag referente ordine sollecito</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="abi">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="10"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="cab">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="10"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="nrCc">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="30"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="flPriCc" type="flagType"/>
	<xsd:element name="cin">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="1"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:complexType name="ccType">
		<xsd:sequence>
			<xsd:element ref="abi"/>
			<xsd:element ref="cab"/>
			<xsd:element ref="nrCc"/>
			<xsd:element ref="cin" minOccurs="0"/>
			<xsd:element ref="flPriCc"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="cdModPag" type="cdType"/>
	<xsd:element name="cdCliente" type="cdType"/>
	<xsd:element name="entFrn">
		<xsd:annotation>
			<xsd:documentation>Condizioni con il fornitore</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="cdCliente" minOccurs="0"/>
				<xsd:element ref="cdModPag" minOccurs="0"/>
				<xsd:element ref="pcSconto" minOccurs="0"/>
				<xsd:element ref="pcMagg" minOccurs="0"/>
				<xsd:element ref="nrGgCons" minOccurs="0"/>
				<xsd:element ref="cdValuta"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="pcSconto" type="xsd:float" fixed="2"/>
	<xsd:element name="pcMagg" type="xsd:float" fixed="2"/>
	<xsd:element name="nrGgCons" type="xsd:integer"/>
	<xsd:element name="cdValuta" type="cdType"/>
	<xsd:element name="bibFrn">
		<xsd:annotation>
			<xsd:documentation>Condizioni fra la Biblioteca e un fornitore</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="relType">
					<xsd:sequence>
						<xsd:element ref="cdBiblioteca"/>
						<xsd:element ref="cdFrn"/>
						<xsd:element ref="entFrn"/>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="polFrn">
		<xsd:annotation>
			<xsd:documentation>Condizioni fra il polo e un fornitore</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="relType">
					<xsd:sequence>
						<xsd:element ref="entFrn"/>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="CFPiva">
		<xsd:annotation>
			<xsd:documentation>Codice fiscale o partita iva. usato solo per i fornitori</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="16"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="gesTut" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Gestione Tipo Utente: valori possibili POL o BIB</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="gesTpv" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Gestione Tipo Provenienza: valori possibili POL o BIB</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="gesAin" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Gestione Aree di interesse: valori poss. POL o BIB</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="idUsername" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Codice identificazione utente. Valori ammessi CF, MA, AC, CU. Per ora (13/10/2004) NON RECUPERATO</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- ======================= AskFrn ========================= -->
	<xsd:complexType name="AskFrn">
		<xsd:annotation>
			<xsd:documentation>Richiesta di anagrafica fornitori</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="dataInizio" type="SbnData"/>
			<xsd:element name="dataFine" type="SbnData"/>
			<xsd:element name="operazione" default="T" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="T"/>
						<xsd:enumeration value="M"/>
						<xsd:enumeration value="C"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="tipoFrn" type="codiceUno" minOccurs="0" maxOccurs="10"/>
		</xsd:sequence>
		<xsd:attribute name="kbibl">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:length value="2"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
</xsd:schema>

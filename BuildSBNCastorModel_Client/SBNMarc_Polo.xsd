		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="80"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="email">
		<xsd:annotation>
			<xsd:documentation>indirizzo email</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="80"/>
				<xsd:minLength value="0"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="indirizzo" type="indirType">
		<xsd:annotation>
			<xsd:documentation>indirizzo</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="indirType">
		<xsd:all>
			<xsd:element name="via1">
				<xsd:annotation>
					<xsd:documentation>indirizzo riga 1</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="0"/>
						<xsd:maxLength value="80"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="cap" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>codice di avviamento postale</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="citta" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>citta</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="siglaProv" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>singla provincia</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="10"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="cdPaese">
				<xsd:annotation>
					<xsd:documentation>codice paese</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="10"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element ref="tel" minOccurs="0"/>
			<xsd:element ref="fax" minOccurs="0"/>
		</xsd:all>
	</xsd:complexType>
	<xsd:element name="note">
		<xsd:annotation>
			<xsd:documentation>note sul fornitore</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="noteType">
				<xsd:minLength value="0"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="cdTpBbr" type="cdType">
		<xsd:annotation>
			<xsd:documentation>tipo bibliotecario</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="pol">
		<xsd:annotation>
			<xsd:documentation>polo</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:all>
				<xsd:element ref="datiAna"/>
				<xsd:element ref="cdPol"/>
				<xsd:element ref="note" minOccurs="0"/>
			</xsd:all>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="posta">
		<xsd:annotation>
			<xsd:documentation>posta</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="30"/>
				<xsd:minLength value="0"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="cdPol" type="cdType">
		<xsd:annotation>
			<xsd:documentation>codice polo</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cdBib" type="cdType">
		<xsd:annotation>
			<xsd:documentation>codice biblioteca</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cdUte" type="cdType">
		<xsd:annotation>
			<xsd:documentation>codice utente</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cdBbr" type="cdType">
		<xsd:annotation>
			<xsd:documentation>codice bibliotecario</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cdGrp" type="cdType">
		<xsd:annotation>
			<xsd:documentation>codice gruppo di utenti</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cdSts" type="cdType">
		<xsd:annotation>
			<xsd:documentation>codice sottosistema</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cdGue" type="cdType">
		<xsd:annotation>
			<xsd:documentation>codice guest</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="cdRecSebina4" type="cdType">
		<xsd:annotation>
			<xsd:documentation>codice recuperato da sebina4 - viene compilato nella conversione dei file per l'import da sebina4</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="rel">
		<xsd:annotation>
			<xsd:documentation>relazioni esportate e importare in file a parte</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="bibUte" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>relazione tra l'utente e la biblioteca</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:complexContent>
							<xsd:extension base="relType">
								<xsd:sequence>
									<xsd:element ref="cdBiblioteca"/>
									<xsd:element ref="cdUte"/>
									<xsd:element ref="cdTpUte" minOccurs="0"/>
									<xsd:element ref="cdTpPro" minOccurs="0"/>
									<xsd:element ref="cdAin" minOccurs="0" maxOccurs="unbounded"/>
								</xsd:sequence>
							</xsd:extension>
						</xsd:complexContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="grpUte" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>relazione tra l'utente e il gruppo</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:complexContent>
							<xsd:extension base="relType">
								<xsd:sequence>
									<xsd:element ref="cdGrp"/>
									<xsd:element ref="cdUte"/>
								</xsd:sequence>
							</xsd:extension>
						</xsd:complexContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="stsBib" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>relazione tra la biblioteca e il sottosistema</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:complexContent>
							<xsd:extension base="relType">
								<xsd:sequence>
									<xsd:element ref="cdBiblioteca"/>
									<xsd:element ref="cdSts"/>
								</xsd:sequence>
							</xsd:extension>
						</xsd:complexContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="bibBbr" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>relazione tra il bibliotecario e la biblioteca</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:complexContent>
							<xsd:extension base="relType">
								<xsd:sequence>
									<xsd:element ref="cdBiblioteca"/>
									<xsd:element ref="cdBbr"/>
								</xsd:sequence>
							</xsd:extension>
						</xsd:complexContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="grbBbr" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>relazione tra il bibliotecario e il gruppo</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:complexContent>
							<xsd:extension base="relType">
								<xsd:sequence>
									<xsd:element ref="cdGrb"/>
									<xsd:element ref="cdBbr"/>
								</xsd:sequence>
							</xsd:extension>
						</xsd:complexContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="stsFrn" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Condizioni fra il sottosistema e un fornitore</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:complexContent>
							<xsd:extension base="relType">
								<xsd:sequence>
									<xsd:element ref="cdSts"/>
									<xsd:element ref="cdFrn"/>
									<xsd:element ref="entFrn"/>
								</xsd:sequence>
							</xsd:extension>
						</xsd:complexContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element ref="bibFrn" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="dtRel">
		<xsd:annotation>
			<xsd:documentation>data della relazione nel formato gg/mm/aaaa</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="10"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:element name="numero">
		<xsd:annotation>
			<xsd:documentation>numero</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:minLength value="0"/>
				<xsd:maxLength value="20"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<xsd:simpleType name="cdType">

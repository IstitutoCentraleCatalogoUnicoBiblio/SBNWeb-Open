			<xsd:enumeration value="RT"/>
			<xsd:enumeration value="BT"/>
			<xsd:enumeration value="NT"/>
			<xsd:enumeration value="UF+"/>
			<xsd:enumeration value="HSF"/>
			<xsd:enumeration value="USE+"/>
			<xsd:enumeration value="HSEE"/>
			<xsd:enumeration value="431"/>

<digester-rules>
    <object-create-rule
    	pattern="labels"
    	classname="java.util.ArrayList"/>
    <pattern value="labels/label">
        <object-create-rule
                classname="it.iccu.sbn.web.actions.gestionesemantica.utility.LabelGestioneSemanticaVO"/>
        <set-properties-rule/>
        <set-next-rule methodname="add"/>
    </pattern>

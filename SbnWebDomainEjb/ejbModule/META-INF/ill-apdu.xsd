	<xs:element name="Shipped-via">
		<xs:annotation>
			<xs:documentation>informazioni sulla modalità di spedizione</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="Physical-delivery" minOccurs="0"/>
				<xs:element ref="Electronic-address" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Sponsoring-body" type="xs:string">
		<xs:annotation>
			<xs:documentation> ente curatore</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Standard-number">
		<xs:annotation>
			<xs:documentation>n. standard </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="Standard-type" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:NMTOKEN">
								<xs:enumeration value="A"/>
								<xs:enumeration value="B"/>
								<xs:enumeration value="C"/>
								<xs:enumeration value="D"/>
								<xs:enumeration value="E"/>
								<xs:enumeration value="I"/>
								<xs:enumeration value="J"/>
								<xs:enumeration value="L"/>
								<xs:enumeration value="M"/>
								<xs:enumeration value="P"/>
								<xs:enumeration value="R"/>
								<xs:enumeration value="S"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="Standard-code" type="xs:string"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Status-or-error-report">
		<xs:annotation>
			<xs:documentation>richiesta di informazioni o invio di errore</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="Transaction-Id" minOccurs="0"/>
				<xs:element ref="Service-date-time" minOccurs="0"/>
				<xs:element ref="Requester-Id" minOccurs="0"/>
				<xs:element ref="Responder-Id" minOccurs="0"/>
				<xs:element ref="Status-report" minOccurs="0"/>
				<xs:element ref="Error-report" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Status-query">
		<xs:annotation>
			<xs:documentation> richiesta di comunicazione dello stato attuale di una richiesta</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Transaction-Id"/>
				<xs:element ref="Service-date-time"/>
				<xs:element ref="Requester-Id"/>
				<xs:element ref="Responder-Id"/>
				<xs:element ref="Note" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Status-report">
		<xs:annotation>
			<xs:documentation>report sullo stato della richiesta</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="History-report" minOccurs="0"/>
				<xs:element ref="Current-state" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Street-and-number" type="xs:string">
		<xs:annotation>
			<xs:documentation>indirizzo e numero civico</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Sub-title" type="xs:string">
		<xs:annotation>
			<xs:documentation>sottotitolo</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Supply-date">
		<xs:annotation>
			<xs:documentation>data in cui sarà disponibile il documento</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="Date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Supply-details">
		<xs:annotation>
			<xs:documentation>contiene i dettagli della spedizione</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence minOccurs="0">
				<xs:element ref="Date-shipped"/>
				<xs:element ref="Date-due"/>
				<xs:element ref="Chargeable-units" minOccurs="0"/>
				<xs:element ref="Cost" minOccurs="0"/>
				<xs:element ref="Shipped-via" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Supply-medium-type">
		<xs:annotation>
			<xs:documentation>contiene il tipo di fornitura richiesto
			00=Fotocopie
			01=Copia di file su floppy disc 
			02=Stampa su carta di file da banche dati 
			03=Fotocopie da microfilm
			04=Fotografie
			05=Documento Originale 
			07=Formato Elettronico 
			08=copia di file su cd-rom 
			09=Scansione digitale
			10=Scansione digitale da microfilm
			11=Stampa da catalogo, cd rom, file digitale 
			12=Stampa su carta fotografica da file digitale 
			13=Altro
</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="SupplyTyp">
						<xs:simpleType>
							<xs:restriction base="xs:NMTOKEN">
								<xs:enumeration value="00"/>
								<xs:enumeration value="02"/>
								<xs:enumeration value="01"/>
								<xs:enumeration value="03"/>
								<xs:enumeration value="04"/>
								<xs:enumeration value="05"/>
								<xs:enumeration value="07"/>
								<xs:enumeration value="08"/>
								<xs:enumeration value="09"/>
								<xs:enumeration value="10"/>
								<xs:enumeration value="11"/>
								<xs:enumeration value="12"/>
								<xs:enumeration value="13"/>
								<xs:enumeration value="1"/>
								<xs:enumeration value="2"/>
								<xs:enumeration value="3"/>
								<xs:enumeration value="4"/>
								<xs:enumeration value="5"/>
								<xs:enumeration value="6"/>
								<xs:enumeration value="7"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="System-no" type="xs:string">
		<xs:annotation>
			<xs:documentation>altro codice identificativo del documento</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Time" type="xs:string">
		<xs:annotation>
			<xs:documentation>ora</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Title" type="xs:string">
		<xs:annotation>
			<xs:documentation>titolo</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Title-of-article" type="xs:string">
		<xs:annotation>
			<xs:documentation>titolo articolo</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Transaction-Id">
		<xs:annotation>
			<xs:documentation>identificativo della transazione</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="institution-symbol" type="xs:string"/>
					<xs:attribute name="name-of-institution" type="xs:string"/>
					<xs:attribute name="transaction-group-qualifier" type="xs:string"/>
					<xs:attribute name="transaction-qualifier" type="xs:string"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Unfilled">
		<xs:annotation>
			<xs:documentation>richiesta non soddisfacibile</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="id" type="xs:string" fixed="F1212"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Verification-reference-source" type="xs:string">
		<xs:annotation>
			<xs:documentation>fonte di provenienza</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Volume-issue" type="xs:string">
		<xs:annotation>
			<xs:documentation>n. volume</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Will-supply">
		<xs:annotation>
			<xs:documentation>attivazione richiesta sospesa</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Supply-date"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:string" fixed="F1214"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Year" type="xs:string">
		<xs:annotation>
			<xs:documentation>anno della data</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Year-issue" type="xs:string">
		<xs:annotation>
			<xs:documentation> anno fascicolo</xs:documentation>
		</xs:annotation>
	</xs:element>
</xs:schema>

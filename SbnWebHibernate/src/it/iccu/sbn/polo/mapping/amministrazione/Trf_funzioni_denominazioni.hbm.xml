<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="it.iccu.sbn.polo.orm.amministrazione.Trf_funzioni_denominazioni" table="trf_funzioni_denominazioni" >
		<meta attribute="class-description">
			DESCRIZIONI IN LINGUA DELLE FUNZIONI GESTITE DALL'APPLICATIVO (--)
		</meta>
		<composite-id>
			<key-many-to-one name="cd_funzione" column="cd_funzione" class="it.iccu.sbn.polo.orm.amministrazione.Tbf_attivita">
			</key-many-to-one>
			<key-many-to-one name="cd_lingua" column="cd_lingua" class="it.iccu.sbn.polo.orm.amministrazione.Tbf_lingua_supportata">
			</key-many-to-one>
		</composite-id>
		<property name="ds_nome" column="ds_nome" type="string" length="100" not-null="true" lazy="false">
			<meta attribute="field-description">
				Descrizione estesa, nella lingua indicata dal relativo codice, della funzione codificata
			</meta>
		</property>
		<property name="ds_nome_breve" column="ds_nome_breve" type="string" length="30" not-null="false" lazy="false">
			<meta attribute="field-description">
				Descrizione breve, nella lingua indicata dal relativo codice, della funzione codificata
			</meta>
		</property>
		<property name="ute_ins" column="ute_ins" type="string" length="12" not-null="true" lazy="false">
			<meta attribute="field-description">
				Utente che ha effettuato l'inserimento
			</meta>
		</property>
		<property name="ts_ins" column="ts_ins" type="timestamp" length="8" not-null="true" lazy="false">
			<meta attribute="field-description">
				Timestamp di inserimento
			</meta>
		</property>
		<property name="ute_var" column="ute_var" type="string" length="12" not-null="true" lazy="false">
			<meta attribute="field-description">
				Utente che ha effettuato la variazione
			</meta>
		</property>
		<property name="ts_var" column="ts_var" type="timestamp" length="8" not-null="true" lazy="false">
			<meta attribute="field-description">
				Timestamp di variazione
			</meta>
		</property>
		<property name="fl_canc" column="fl_canc" type="character" length="1" not-null="true" lazy="false">
			<meta attribute="field-description">
				Flag di cancellazione logica
			</meta>
		</property>
	</class>
</hibernate-mapping>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="it.iccu.sbn.polo.orm.bibliografica.Tb_impronta" table="tb_impronta" >
		<meta attribute="class-description">
			IMPRONTE
		</meta>
		<composite-id>
			<key-many-to-one name="B" column="bid" class="it.iccu.sbn.polo.orm.bibliografica.Tb_titolo">
			</key-many-to-one>
			<key-property name="impronta_1" column="impronta_1" type="string"/>
			<key-property name="impronta_2" column="impronta_2" type="string"/>
			<key-property name="impronta_3" column="impronta_3" type="string"/>
		</composite-id>
		<property name="nota_impronta" column="nota_impronta" type="string" length="80" not-null="false" lazy="false">
			<meta attribute="field-description">
				Nota al collegamento titolo impronta
			</meta>
		</property>
		<property name="ute_ins" column="ute_ins" type="string" length="12" not-null="true" lazy="false">
			<meta attribute="field-description">
				Utente che ha effettuato l'inserimento
			</meta>
		</property>
		<property name="ts_ins" column="ts_ins" type="timestamp" length="8" not-null="true" lazy="false">
			<meta attribute="field-description">
				Timestamp di inserimento
			</meta>
		</property>
		<property name="ute_var" column="ute_var" type="string" length="12" not-null="true" lazy="false">
			<meta attribute="field-description">
				Utente che ha effettuato la variazione
			</meta>
		</property>
		<property name="ts_var" column="ts_var" type="timestamp" length="8" not-null="true" lazy="false">
			<meta attribute="field-description">
				Timestamp di variazione
			</meta>
		</property>
		<property name="fl_canc" column="fl_canc" type="character" length="1" not-null="true" lazy="false">
			<meta attribute="field-description">
				Flag di cancellazione logica
			</meta>
		</property>
	</class>
</hibernate-mapping>

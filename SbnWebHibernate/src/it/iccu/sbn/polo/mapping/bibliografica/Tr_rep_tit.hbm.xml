<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="it.iccu.sbn.polo.orm.bibliografica.Tr_rep_tit" table="tr_rep_tit" >
		<meta attribute="class-description">
			COLLEGAMENTO TITOLO REPERTORIO
		</meta>
		<composite-id>
			<key-many-to-one name="B" column="bid" class="it.iccu.sbn.polo.orm.bibliografica.Tb_titolo">
			</key-many-to-one>
			<key-many-to-one name="Id_repertorio" column="id_repertorio" class="it.iccu.sbn.polo.orm.bibliografica.Tb_repertorio">
			</key-many-to-one>
		</composite-id>
		<property name="nota_rep_tit" column="nota_rep_tit" type="string" length="160" not-null="false" lazy="false"/>
		<property name="fl_trovato" column="fl_trovato" type="character" length="1" not-null="true" lazy="false"/>
		<property name="ute_ins" column="ute_ins" type="string" length="12" not-null="true" lazy="false">
			<meta attribute="field-description">
				Utente che ha effettuato l'inserimento
			</meta>
		</property>
		<property name="ts_ins" column="ts_ins" type="timestamp" length="8" not-null="true" lazy="false">
			<meta attribute="field-description">
				Timestamp di inserimento
			</meta>
		</property>
		<property name="ute_var" column="ute_var" type="string" length="12" not-null="true" lazy="false">
			<meta attribute="field-description">
				Utente che ha effettuato la variazione
			</meta>
		</property>
		<property name="ts_var" column="ts_var" type="timestamp" length="8" not-null="true" lazy="false">
			<meta attribute="field-description">
				Timestamp di variazione
			</meta>
		</property>
		<property name="fl_canc" column="fl_canc" type="character" length="1" not-null="true" lazy="false">
			<meta attribute="field-description">
				Flag di cancellazione logica
			</meta>
		</property>
	</class>
</hibernate-mapping>
